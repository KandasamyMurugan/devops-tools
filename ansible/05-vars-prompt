- name: Prompting Variables
- hosts: all
  vars_prompt: 
    - prompt: Enter URL
      name: URL
      private: yes

  tasks:
    - name: Print URL
      debug:
        msg: "URL = {{URL}}"
  vars_prompt:
    - prompt: "What is your username?"
      name: username
      private: no
  tasks:
       debug:
        msg: 'Logging in as {{ username }}'